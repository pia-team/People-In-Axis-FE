# Toggle authentication (Keycloak)
# true  -> Keycloak enabled (default)
# false -> Auth bypassed for development/testing
VITE_AUTH_ENABLED=false

# API base URL
VITE_API_BASE_URL=http://localhost:8080/api
# API version prefix (optional). Examples:
# VITE_API_VERSION=v1   -> requests go to /api/v1/*
# VITE_API_VERSION=     -> requests go to /api/*
VITE_API_VERSION=

# Keycloak settings
VITE_KEYCLOAK_URL=http://localhost:8180
VITE_KEYCLOAK_REALM=people-in-axis
VITE_KEYCLOAK_CLIENT_ID=people-in-axis-frontend
